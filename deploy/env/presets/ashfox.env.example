# Ashfox managed preset
ASHFOX_PERSISTENCE_PRESET=ashfox
ASHFOX_PERSISTENCE_FAIL_FAST=true

# Either provide a full URL:
# ASHFOX_DB_ASHFOX_URL=postgresql://postgres:secret@database.sigee.xyx:5432/postgres?sslmode=require
# Or compose from host/user fields:
ASHFOX_DB_ASHFOX_HOST=database.sigee.xyx
ASHFOX_DB_ASHFOX_PORT=5432
ASHFOX_DB_ASHFOX_USER=postgres
ASHFOX_DB_ASHFOX_PASSWORD=replace-me
ASHFOX_DB_ASHFOX_NAME=postgres
ASHFOX_DB_ASHFOX_SSL=true
ASHFOX_DB_ASHFOX_SCHEMA=public
ASHFOX_DB_ASHFOX_TABLE=ashfox_projects
ASHFOX_DB_ASHFOX_MIGRATIONS_TABLE=ashfox_schema_migrations

ASHFOX_STORAGE_ASHFOX_URL=https://database.sigee.xyx
ASHFOX_STORAGE_ASHFOX_SERVICE_KEY=replace-me
ASHFOX_STORAGE_ASHFOX_UPSERT=true
# ASHFOX_STORAGE_ASHFOX_KEY_PREFIX=
