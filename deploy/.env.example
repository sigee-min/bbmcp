# Image tags
ASHFOX_WEB_IMAGE=ashfox/web:local
ASHFOX_GATEWAY_IMAGE=ashfox/mcp-gateway:local
ASHFOX_WORKER_IMAGE=ashfox/worker:local

# Published host ports
ASHFOX_WEB_PORT=8686
ASHFOX_GATEWAY_PORT=8787
ASHFOX_GATEWAY_PATH=/mcp
ASHFOX_POSTGRES_PORT=5432

# Runtime mode
ASHFOX_GATEWAY_BACKEND=engine
ASHFOX_PERSISTENCE_PRESET=selfhost
ASHFOX_DB_POSTGRES_URL=postgresql://ashfox:ashfox@postgres:5432/ashfox

# Worker timing
ASHFOX_WORKER_HEARTBEAT_MS=5000
ASHFOX_WORKER_POLL_MS=1200
ASHFOX_WORKER_NATIVE_PIPELINE=1

# Postgres bootstrap credentials
POSTGRES_DB=ashfox
POSTGRES_USER=ashfox
POSTGRES_PASSWORD=ashfox
