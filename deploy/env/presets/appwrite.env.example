# Appwrite managed preset (Databases + Storage)
ASHFOX_PERSISTENCE_PRESET=appwrite
ASHFOX_PERSISTENCE_FAIL_FAST=true

ASHFOX_DB_APPWRITE_URL=https://cloud.appwrite.io/v1
ASHFOX_DB_APPWRITE_PROJECT_ID=replace-me
ASHFOX_DB_APPWRITE_API_KEY=replace-me
ASHFOX_DB_APPWRITE_DATABASE_ID=ashfox
ASHFOX_DB_APPWRITE_COLLECTION_ID=ashfox_projects
# Optional:
# ASHFOX_DB_APPWRITE_TIMEOUT_MS=15000
# ASHFOX_DB_APPWRITE_RESPONSE_FORMAT=1.8.0

ASHFOX_STORAGE_APPWRITE_URL=https://cloud.appwrite.io/v1
ASHFOX_STORAGE_APPWRITE_PROJECT_ID=replace-me
ASHFOX_STORAGE_APPWRITE_API_KEY=replace-me
ASHFOX_STORAGE_APPWRITE_BUCKET_ID=ashfox_blobs
ASHFOX_STORAGE_APPWRITE_UPSERT=true
# Optional:
# ASHFOX_STORAGE_APPWRITE_KEY_PREFIX=
# ASHFOX_STORAGE_APPWRITE_METADATA_DATABASE_ID=ashfox
# ASHFOX_STORAGE_APPWRITE_METADATA_COLLECTION_ID=ashfox_blob_metadata
# ASHFOX_STORAGE_APPWRITE_TIMEOUT_MS=15000
# ASHFOX_STORAGE_APPWRITE_RESPONSE_FORMAT=1.8.0
